  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DutsAndrew/blog-api/issues"
  },
  "homepage": "https://github.com/DutsAndrew/blog-api",
  "dependencies": {
    "async": "^3.2.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.21",
