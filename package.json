    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "concurrently": "^7.6.0",
    "typescript": "^5.0.2"
  }
